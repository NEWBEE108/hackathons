<?xml version="1.0" encoding="utf-8"?>
<manifest package="azoni.likeithikeit"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity
            android:name=".TrailsActivity"
            android:label="@string/title_activity_trails"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".CalenderActivity">
        </activity>
        <activity android:name=".JoinActivity">
        </activity>
        <activity android:name=".CreateActivity">
        </activity>
        <activity android:name=".ConfirmActivity">
        </activity>
        <activity android:name=".ViewCalenderActivity">
        </activity>
    </application>

</manifest>